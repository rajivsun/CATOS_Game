function enemy_gethit(){
	if hit{
	path_end();
	speed = lerp(speed,0,0.1);
	if place_meeting(x + lengthdir_x(speed,direction),y +lengthdir_y(speed,direction),obj_concrete)
		speed = 0;
	exit;
}

}